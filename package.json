{
  "name": "iso-test",
  "version": "1.3.8",
  "description": "Isomorphic test framework for browser & node libs and apps.",
  "main": "index.node.js",
  "module": "index.js",
  "esm": true,
  "bin": {
    "iso-test": "./server.node.js"
  },
  "scripts": {
    "test": "node ./test.js",
    "gpminstall": "npm i -g https://github.com/isysd-mirror/gpm.git",
    "preinstall": "gpm -n .. -t .. -u https -d 1 -i .",
    "postinstall": "node -e \"try { require('../esm/esm.js')(module); } catch (e) {require('child_process').execSync('npm i', {cwd: require('path').join('..', 'esm')})}\""
  },
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "dotenv": "../dotenv",
    "esm": "../esm",
    "open": "../open",
    "iso-process": "../iso-process",
    "tree-kill": "../tree-kill"
  },
  "peerDependencies": {
    "gpm": "https://github.com/isysd-mirror/gpm.git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isysd-mirror/iso-test.git"
  },
  "keywords": [
    "isomorphic",
    "test",
    "nodejs",
    "browser",
    "firefox",
    "chrome",
    "chromium",
    "safari"
  ],
  "homepage": "https://github.com/isysd-mirror/iso-test/"
}
